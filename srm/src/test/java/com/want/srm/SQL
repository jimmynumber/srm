drop table if exists FACTORY;
CREATE TABLE  FACTORY
    (
        `ID` BIGINT(18) NOT NULL AUTO_INCREMENT,
        FACTORY_ID VARCHAR(50) COMMENT '工厂编号',
        FACTORY_NAME VARCHAR(50) COMMENT '工厂描述',
        PRIMARY KEY (ID)
    )
    ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8   
    
    
    drop table if exists ATTRIBUTE_INFO;
CREATE TABLE  ATTRIBUTE_INFO
    (
        ATTRIBUTE_KEY VARCHAR(50) NOT NULL ,
        ATTRIBUTE_VALUE VARCHAR(500) ,
        ATTRIBUTE_ID VARCHAR(50) NOT NULL ,
        ATTRIBUTE_NAME VARCHAR(50) ,
        STATUS CHAR(1) default 1 ,
        UPDATOR VARCHAR(20) default 'MIS' ,
        UPDATE_DATE TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
        PRIMARY KEY (ATTRIBUTE_KEY, ATTRIBUTE_ID)
    )
    ENGINE=InnoDB DEFAULT CHARSET=utf8  
    
CREATE TABLE
    FILE_UPLOAD
    (
        ID bigint(18) NOT NULL AUTO_INCREMENT,
        FILE_NAME VARCHAR(100),
        FILE_DESC VARCHAR(100),
        ORI_FILE_NAME VARCHAR(100),
        FILE_TYPE VARCHAR(20),
        FILE_SIZE VARCHAR(20),
        FILE_PATH VARCHAR(400),
        FILE_STATUS CHAR(1) DEFAULT '1',
        CREATOR VARCHAR(50) DEFAULT 'MIS',
        CREATE_DATE TIMESTAMP DEFAULT CURRENT_TIMESTAMP NULL,
        NOTICECODE VARCHAR(100),
        PRIMARY KEY (ID)
    )
    ENGINE=InnoDB DEFAULT CHARSET=utf8




    
    










    
    
    